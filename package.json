{
  "name": "rmwapi",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel src -d build",
    "start": "node ./build/app.js",
    "start:dev": "nodemon --exec babel-node src/app.js",
    "go": "npm run build && npm start",
    "test:build": "nyc --reporter=html --reporter=text mocha ./build/test/*.test.js  --exit",
    "test": "nyc --reporter=html --reporter=text mocha ./src/test/*.test.js  --compilers js:babel-core/register --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "migrate": "babel-node ./src/models/databaseMigrations",
    "migration": "node ./build/models/databaseMigrations"
  },
  "nyc": {
    "exclude": [
      "**/*.test.js",
      "**/src/config/**"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "istanbul": "*",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2",
    "should": "^13.2.1"
  }
}
